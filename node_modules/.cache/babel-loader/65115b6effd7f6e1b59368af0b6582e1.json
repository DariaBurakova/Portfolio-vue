ceholder=\"Date\" v-model=\"date\"/>\n    <input  class=\"points\" placeholder=\"Category\"  v-model=\"category\"/>\n    <input  class=\"points\" placeholder=\"Value\"   v-model.number=\"value\"/>\n    <button class=\"save\" @click=\"onSaveClick\">ADD +</button>\n  </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'AddPaymentForm',\ndata(){\n    return{\n      date:'',\n      category:'',\n      value:null,\n      show:false,\n    }\n},\n  computed:{\n    getCurrentDate () {\n      const today = new Date();\n      const d = today.getDate()\n      const m = today.getMonth() + 1\n      const y = today.getFullYear()\n      return `${d}.${m}.${y}`\n    }\n  },\n  methods:{\n    onSaveClick () {\n      const data = {\n        date: this.date || this.getCurrentDate,\n        category: this.category,\n        value: this.value,\n      }\n      this.$emit('addNewPayment', data)\n    }\n  },\n}\n</script>\n\n<style lang=\"scss\" scoped >\n.addShow{\n  border:none;\n  padding:5px 20px;\n  background-color: #66CDAA;\n  font-weight:bold;\n  border:solid 1px #dad8d5;\n  color:#FFFFFF;\n}\n.form{\n  display:flex;\n  flex-direction:column;\n  align-items:flex-start;\n}\n.points{\n  margin-top:15px;\n  padding:5px;\n  box-shadow: rgba(0,0,0,.5) 3px 10px 10px -10px;\n  border:solid 1px #dad8d5;\n}\n.save{\n  margin-top:15px;\n  padding:5px 10px;\n  background-color: #66CDAA;\n  border:solid 1px #dad8d5;\n  font-weight:bold;\n  color:#FFFFFF;\n}\n</style>"],"sourceRoot":"src/components"}]}