tCA,qBAsCA;AACA,QAAA,CAAA,KAAA,eAAA,CAAA,MAAA,EAAA;AACA,WAAA,cAAA;AACA;AACA,GA1CA;AA2CA,EAAA,OAAA;AACA,IAAA,WADA,yBACA;AACA,UAAA,IAAA,GAAA;AACA,QAAA,EAAA,EAAA,KAAA,eAAA,CAAA,MAAA,GAAA,CADA;AAEA,QAAA,IAAA,EAAA,KAAA,IAAA,IAAA,KAAA,cAFA;AAGA,QAAA,QAAA,EAAA,KAAA,QAAA,IAAA,KAAA,sBAHA;AAIA,QAAA,KAAA,EAAA,KAAA,KAAA,IAAA,KAAA;AAJA,OAAA;AAMA,WAAA,MAAA,CAAA,MAAA,CAAA,uBAAA,EAAA,IAAA;AACA;AATA,KAUA,UAAA,CAAA,CACA,gBADA,CAAA,CAVA;AA3CA,CAAA","sourcesContent":["<template>\n<div class=\"container\">\n  <div class=\"form \">\n    <h1 >My personal costs</h1>\n  <button class=\"addShow\" @click=\"show=!show\">ADD NEW COST +</button>\n  <div v-show=\"show\" class=\"form\" >\n    <input class=\"points\" placeholder=\"Date\" v-model=\"date\"/>\n    <input  class=\"points\" placeholder=\"Category\" v-model=\"category\" />\n    <select v-model=\"category\" class=\"points\" >\n      <option v-for =\"option in getCategoryList\" :key=\"option\"  >{{option}}</option>\n    </select>\n    <input  class=\"points\" placeholder=\"Value\"   v-model.number=\"value\"/>\n    <button class=\"save\" @click=\"onSaveClick\">ADD +</button>\n  </div>\n  </div>\n</div>\n\n</template>\n\n<script>\nimport {mapMutations, mapGetters,mapActions} from 'vuex'\n\n\nexport default {\n  name: 'AddPaymentForm',\n  data(){\n    return{\n      date:'',\n      category:'',\n      value:null,\n      show:false,\n    }\n\n},\n  computed:{\n    ...mapMutations([\n      'addDataToPaymentsList',\n    ]),\n    ...mapGetters([\n      'getCategoryList',\n        'getPaymentsList'\n    ]),\n    getCurrentDate () {\n      const today = new Date();\n      const d = today.getDate()\n      const m = today.getMonth() + 1\n      const y = today.getFullYear()\n      return `${d}.${m}.${y}`\n    },\n    getParamRouterCategory (){\n      console.log(this.$route.params.category)\n      const categoryRoute = this.$route.params.category\n      return categoryRoute\n    },\n    getParamRouterValue(){\n      console.log(this.$route.params.value)\n      const valueRoute = this.$route.params.value\n      return valueRoute\n    }\n\n  },\n  mounted () {\n    if (!this.getCategoryList.length) {\n      this.loadCategories()\n    }\n  },\n  methods:{\n    onSaveClick () {\n      const data = {\n        id:this.getPaymentsList.length +1,\n        date: this.date || this.getCurrentDate,\n        category: this.category || this.getParamRouterCategory,\n        value: this.value || this.getParamRouterValue\n      }\n      this.$store.commit('addDataToPaymentsList', data)\n    },\n    ...mapActions([\n      'loadCategories'\n    ]),\n\n\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped >\n.container{\n  margin:0 auto;\n  width: 400px;\n}\n.addShow{\n  border:none;\n  padding:5px 20px;\n  background-color: #66CDAA;\n  font-weight:bold;\n  color:#FFFFFF;\n\n}\n.addShow:hover{\n  transform: scale(1.01);\n  box-shadow: 0 14px 28px rgba(0,0,0,0.25), 0 10px 10px rgba(0,0,0,0.22);\n}\n.form{\n  display:flex;\n  flex-direction:column;\n  align-items:flex-start;\n}\n.points{\n  margin-top:15px;\n  padding:5px;\n  box-shadow: rgba(0,0,0,.5) 3px 10px 10px -10px;\n  border:solid 1px #dad8d5;\n}\n.points:hover{\n  transform: scale(1.01);\n  box-shadow: 0 14px 28px rgba(0,0,0,0.25), 0 10px 10px rgba(0,0,0,0.22);\n}\n.save{\n  margin-top:15px;\n  padding:5px 10px;\n  background-color: #66CDAA;\n  border:solid 1px #dad8d5;\n  font-weight:bold;\n  color:#FFFFFF;\n}\n.save:hover{\n  transform: scale(1.01);\n  box-shadow: 0 14px 28px rgba(0,0,0,0.25), 0 10px 10px rgba(0,0,0,0.22);\n}\n\n</style>"],"sourceRoot":"src/components"}]}