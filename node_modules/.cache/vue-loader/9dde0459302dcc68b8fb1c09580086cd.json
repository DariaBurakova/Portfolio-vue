{"remainingRequest":"/Users/daria/Documents/Vue2/vue-project/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/daria/Documents/Vue2/vue-project/src/components/AddPaymentForm.vue?vue&type=style&index=0&id=36a79239&lang=scss&scoped=true&","dependencies":[{"path":"/Users/daria/Documents/Vue2/vue-project/src/components/AddPaymentForm.vue","mtime":1634721818611},{"path":"/Users/daria/Documents/Vue2/vue-project/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/daria/Documents/Vue2/vue-project/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":499162500000},{"path":"/Users/daria/Documents/Vue2/vue-project/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/Users/daria/Documents/Vue2/vue-project/node_modules/sass-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/daria/Documents/Vue2/vue-project/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/daria/Documents/Vue2/vue-project/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["AddPaymentForm.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuGA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"AddPaymentForm.vue","sourceRoot":"src/components","sourcesContent":["<template>\n<v-container class=\"container\">\n  <div class=\"form \">\n    <h1 >My personal costs</h1>\n  <v-btn color=\"secondary\"\n         elevation=\"4\" class=\"addShow\" @click=\"show=!show\">ADD NEW COST +</v-btn>\n  <v-form v-show=\"show\" class=\"form\" >\n    <v-text-field  class=\"points\" placeholder=\"Date\" v-model=\"date\"></v-text-field>\n    <v-text-field  class=\"points\" placeholder=\"Category\" v-model=\"category\" ></v-text-field>\n    <v-select v-model=\"category\"  class=\"points\" :items=\"getCategoryList\">\n    </v-select>\n    <v-text-field  class=\"points\" placeholder=\"Value\"   v-model.number=\"value\"></v-text-field>\n    <v-btn color=\"secondary\"\n           elevation=\"4\" class=\"save\" @click=\"onSaveClick\">ADD +</v-btn>\n  </v-form>\n  </div>\n</v-container>\n\n</template>\n\n<script>\nimport {mapMutations, mapGetters,mapActions} from 'vuex'\nimport {getCurrentDate} from '../utils'\nimport {select} from '../assets/select'\nexport default {\n  name: 'AddPaymentForm',\n  data(){\n    return{\n      date:'',\n      category:'',\n      value:null,\n      show:false,\n    }\n\n},\n  watch:{\n    '$route'(){\n      this.category = this.getRouteParams.name\n      this.value = this.getRouteParams.params?.value\n    }\n  },\n  computed:{\n    ...mapGetters('payments',[\n      'getCategoryList',\n        'getPaymentsList'\n    ]),\n    list() {\n      return select\n    },\n    getRouteParams() {\n      return {\n        name: this.$route.name,\n        params: this.$route.params\n      }\n    },\n    isEmpty() {\n      return this.date && this.category && this.value\n    }\n  },\n  mounted() {\n    if (!this.getCategoryList.length) {\n      this.loadCategories()\n    }\n    this.setParams()\n  },\n  methods:{\n    ...mapMutations('payments',[\n      'addDataToPaymentsList',\n    ]),\n    getCoincidence() {\n      return this.list.some(el => el.category === this.$route.name)\n    },\n    getCurrentDate,\n    setParams() {\n      if (this.getCoincidence()) {\n        this.date = this.getCurrentDate()\n        this.category = this.$route.name\n        this.value = this.$route.params?.value\n      } else {\n        this.date = null\n        this.category = null\n        this.value = null\n      }\n    },\n    onSaveClick() {\n      const data = {\n        id:this.getPaymentsList.length + 1,\n        date: this.date || this.getCurrentDate(),\n        category: this.category,\n        value:this.value\n      }\n      this.addDataToPaymentsList(data)\n    },\n    ...mapActions('payments',[\n      'loadCategories'\n    ]),\n\n\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped >\n.container{\n  margin:40px auto;\n  width: 400px;\n}\n.addShow{\n  margin-top: 10px;\n\n}\n.form{\n  display:flex;\n  flex-direction:column;\n  align-items:flex-start;\n}\n.points{\n  margin-top:10px;\n  padding:5px;\n\n}\n.points:hover{\n  transform: scale(1.01);\n  box-shadow: 0 14px 28px rgba(0,0,0,0.25), 0 10px 10px rgba(0,0,0,0.22);\n}\n.save{\n  margin-top:15px;\n  padding:5px 10px;\n  background-color: #66CDAA;\n  border:solid 1px #dad8d5;\n  font-weight:bold;\n  color:#FFFFFF;\n}\n.save:hover{\n  transform: scale(1.01);\n  box-shadow: 0 14px 28px rgba(0,0,0,0.25), 0 10px 10px rgba(0,0,0,0.22);\n}\n\n</style>"]}]}